/* pages/my/childCpns/m-list-view/index.wxss */
.list-view{
	border-top: 36rpx solid #f8f8f8;
}
.list-item{
	height: 100rpx;
	line-height: 100rpx;
	font-size: 30rpx;
	margin-left: 30rpx;
	border-bottom: 1px solid #f8f8f8;
}
.list-item:last-child {
  border-bottom: none;
}

.list-item image {
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
}

.list-item text {
  vertical-align: middle;
  margin-left: 16rpx;
}