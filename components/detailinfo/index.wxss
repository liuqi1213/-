.header{
	position: relative;
	padding: 10rpx 30rpx;
	font-size: 32rpx
}
.line{
	width: 180rpx;
	height: 1px;
	background: #666;
	margin: 30rpx 0;
	position: relative;
}

.end{
	float: right;
}
 .header .start::before,.header .end::after{
	content:"" ;
	position: absolute;
	width: 10rpx;
	height: 10rpx;
	background: #333;
}
 .header .start::before{
	 bottom: 0;
 }
 .header .end::after{
	 bottom: 0;
	 right: 0;
 }
 
